<xml>
    <object name="Accels" singleinstance="true" settings="false">
        <description>Raw values from the acceleration sensor. Updated and used by the Attitude module.</description>
    	<field name="x" units="m/s^2" type="FLOAT32"/>
    	<field name="y" units="m/s^2" type="FLOAT32"/>
    	<field name="z" units="m/s^2" type="FLOAT32"/>
    	<access gcs="readonly" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="periodic" period="500"/>
        <logging period="0"/>
    </object>
</xml>
