<xml>
    <object name="FlightBatterySettings" singleinstance="true" settings="true">
        <description>Battery configuration.</description>
        <field name="Type" units="" type="ENUM" options="LiPo,A123,LiCo,LiFeSO4,None" defaultvalue="LiPo"/>
        <field name="Cells" units=""  type="UINT8" defaultvalue="3"/>
        <field name="Capacity" units="mAh"  type="UINT32" defaultvalue="2200"/>
        <field name="VoltageThreshold" units="V"  type="FLOAT32" defaultvalue="9.8"/>
        <field name="SensorCalibration" units="" type="FLOAT32" defaultvalue="1.0"/>
    	<access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="manual" period="0"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
