<xml>
    <object name="Gyros" singleinstance="true" settings="false">
        <description>Raw values from the gyroscope sensor. Updated and used by the Attitude module.</description>
        <field name="x" units="deg/s" type="FLOAT32"/>
        <field name="y" units="deg/s" type="FLOAT32"/>
        <field name="z" units="deg/s" type="FLOAT32"/>
        <access gcs="readonly" flight="readwrite"/>
        <telemetrygcs updatemode="manual" period="0"/>
        <telemetryflight updatemode="periodic" period="1000"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
